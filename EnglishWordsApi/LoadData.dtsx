    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        palabra = Row.palabra;

        // URL base de la API
        string apiUrl = "https://api.dictionaryapi.dev/api/v2/entries/en/";

        string url = apiUrl + palabra;

        // Crear un cliente HTTP
        HttpClient client = new HttpClient();

        try
        {
            // Realizar la llamada a la API de forma as ncrona
            Task<string> response = client.GetStringAsync(url);

            // Esperar la respuesta y procesarla
            string jsonResponse = response.Result;

            // Parsear la respuesta JSON
            JArray wordData = JArray.Parse(jsonResponse);

            string sourceUrl = "";

            // Iterar sobre las definiciones y agregarlas como nuevas filas de salida
            if (wordData != null && wordData.Count > 0)
            {
                foreach (JToken word in wordData)
                {
                    if (word["meanings"] != null)
                    {
                        foreach (JToken phonetic in word["phonetics"])
                        {
                            sourceUrl = phonetic["sourceUrl"] != null ? phonetic["sourceUrl"].ToString() : "";
                        }

                        foreach (JToken meaning in word["meanings"])
                        {
                            foreach (JToken definition in meaning["definitions"])
                            {
                                Output0Buffer.AddRow();
                                Output0Buffer.Word = palabra;
                                Output0Buffer.SourceUrl = sourceUrl;
                                Output0Buffer.PartOfSpeech = meaning["partOfSpeech"] != null ? meaning["partOfSpeech"].ToString() : "";
                                Output0Buffer.Definition = definition["definition"].ToString();
                                Output0Buffer.Example = definition["example"] != null ? definition["example"].ToString() : "";
                            }
                        }
                    }
                }
            }
        }
        catch (Exception ex)
        {
