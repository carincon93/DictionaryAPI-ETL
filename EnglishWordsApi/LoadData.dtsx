            // Console.WriteLine("Error al buscar la definición de la palabra '" + word + "': " + ex.Message);

            // Escribir en el archivo de registro al finalizar el proceso
            string logFilePath = @"C:\Users\Cristian\Downloads\Log.txt";
            using (StreamWriter writer = new StreamWriter(logFilePath, true))
            {
                writer.WriteLine("Palabra: " + palabra);
                writer.Flush();
            }
        }
        finally
        {
            // Cerrar el cliente HTTP
            client.Dispose();
        }
    }
